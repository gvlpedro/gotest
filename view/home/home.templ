package home

templ Home() {
	<section class="overflow-hidden bg-gray-50 sm:grid sm:grid-cols-2 sm:items-center bg-gray-900 mx-auto px-4 sm:px-8">
		<input data-bind-input/>
		<div data-computed-repeated="$input.repeat(2)">
			<input data-bind-input/>
			<div data-text="$repeated">
				Will be replaced with the contents of the repeated signal
			</div>
		</div>
	</section>
}
